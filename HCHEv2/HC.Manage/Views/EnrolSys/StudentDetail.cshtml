@{
    Layout = null;
}
@using HC.Data.Models
@model StudentInfo
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>StudentDetail</title>
    <link rel="stylesheet" href="~/HC.Manage/Content/common/css/Style.css">
</head>
<body>
    <div id="tb_basic0" class="ScrollBar">
        <table border="0" cellpadding="0" cellspacing="0" class="frm show" id="GvCourses">

            <tbody>
                <tr>


                    <th rowspan="5">
                        照片：
                    </th>
                    <td rowspan="5">
                        <img src="@Model.Sphoto" width="150" height="150" />
                    </td>

                </tr>



                <tr>

                    <th>姓名：</th>
                    <td>@Model.StudentName</td>
                </tr>
                <tr>
                    <th> 性别： </th>
                    <td> @Model.Sex </td>
                </tr>
                <tr>
                    <th>身份证：</th>
                    <td>@Model.SelfCardNo</td>
                </tr>
                <tr>

                    <th>
                        高考证号：
                    </th>
                    <td>
                        @Model.registerNo
                    </td>

                </tr>







                <tr>
                    <th> 手机号： </th>
                    <td> @Model.SeflPhone </td>
                    <th> 父亲联系方式：</th>
                    <td>@Model.FatherPhone</td>                    
                </tr>

                <tr>
                    <th> 母亲联系方式： </th>
                    <td> @Model.MotherPhone </td>
                    <th>座机号：</th>
                    <td>@Model.TelNum</td>
                </tr>

                <tr>
                    <th> 学生类型： </th>
                    <td> @Model.Stype </td>
                    <th> 科别： </th>
                    <td> @Model.Scategory</td>
                </tr>

                <tr>
                    <th> 生源地： </th>
                    <td> @Model.Sfrom</td>
                    <th>
                        中学名称：
                    </th>
                    <td>
                        @Model.SchoolName
                    </td>
                </tr>

                <tr>
                    <th> 专业： </th>
                    <td> @Model.major</td>

                    <th> 收件人电话： </th>
                    <td> @Model.ReceivePhone</td>                   
                </tr>

                <tr>
                    <th> 邮编： </th>
                    <td> @Model.ZipCode</td>
                    <th>
                        收件人：
                    </th>
                    <td>
                        @Model.ReceiveName
                    </td>
                </tr>

                <tr>
                    <th>
                        录取通知书邮寄地址：
                    </th>
                    <td colspan="3">
                        @Model.SendAddress
                    </td>
                </tr>

            </tbody>


        </table>
    </div>
</body>
</html>
